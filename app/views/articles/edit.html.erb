<%= provide(:title, raw("Edit \"#{@article.title}\"")) %>
<h1>Edit "<%= @article.title %>"</h1>

<div class="row">
  <div class="offset2">
    <%= form_for(@article) do |f| %>
      <%= render 'articles/form_fields', f: f %> 
      <%= f.submit 'Update', class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>